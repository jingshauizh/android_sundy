<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jingshuai.android.fregmentapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity" />
        <activity
            android:name=".activity.DescActivity"
            android:label="@string/title_activity_desc"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.ListActivity"
            android:label="@string/title_activity_desc"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.TitlesActivity"
            android:label="@string/title_activity_desc"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.MainMenuListActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".test.commonadapter.CommonAdapterActivity" />
        <activity
            android:name=".test.okhttp.OkHttpActivity"
            android:label="@string/title_activity_ok_http"
            android:parentActivityName="com.jingshuai.appcommonlib.activity.ActivityLibBase"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jingshuai.appcommonlib.activity.ActivityLibBase" />
        </activity>
        <activity android:name=".customer.Customer_View_Activity"></activity>
    </application>

</manifest>
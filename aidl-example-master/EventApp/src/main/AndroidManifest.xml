<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eventapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AnimationActivity"></activity>
        <activity
            android:name=".ActvityStateSave"
            android:label="@string/title_activity_actvity_state_save"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ActivityDialog" />
        <activity android:name=".Activityenu" />
        <activity android:name=".activity.ActivitMenu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ActivityEvent" />
        <activity android:name=".activity.AcDialog"
            android:theme="@android:style/Theme.Dialog"
            ></activity>
    </application>

</manifest>